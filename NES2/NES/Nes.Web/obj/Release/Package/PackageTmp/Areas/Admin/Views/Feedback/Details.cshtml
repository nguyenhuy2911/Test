@model Nes.Dal.EntityModels.Feedback

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">@Nes.Resources.NesResource.AdminFeedbackDetailTitlePage</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                @Nes.Resources.NesResource.AdminNoteForm
            </div>
            <div class="panel-body">

                <div class="row">
                    <div class="col-lg-12">
                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.Name)</label>
                            @Html.DisplayFor(model => model.Name)
                        </div>

                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.Company)</label>
                            @Html.DisplayFor(model => model.Company)
                        </div>

                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.Address)</label>
                            @Html.DisplayFor(model => model.Address)
                        </div>

                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.Phone)</label>

                            @Html.DisplayFor(model => model.Phone)
                        </div>

                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.Email)</label>

                            @Html.DisplayFor(model => model.Email)
                        </div>

                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.Title)</label>

                            @Html.DisplayFor(model => model.Title)
                        </div>

                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.Message)</label>

                            @Html.DisplayFor(model => model.Message)
                        </div>

                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.CreatedDate)</label>

                            @Html.DisplayFor(model => model.CreatedDate)
                        </div>

                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.IsReaded)</label>
                            @Html.DisplayFor(model => model.IsReaded)
                        </div>
                        @Html.ActionLink(@Nes.Resources.NesResource.GoBackToListText, "Index");
                    </div>
                </div>
                <!-- /.row (nested) -->
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
